  border: 0px solid transparent;
}
QComboBox#comboLangue  {
  padding: 1px 18px 1px 3px;
  min-width: 6em;
  border: 0px solid yellow;
  border-right: 10px solid #64ffda;
  min-height: 25px;
  font-size: 15px;
  background-color: #262626;
  color: #64ffda;
}
QComboBox#comboTheme {
  padding: 1px 18px 1px 3px;
  min-width: 6em;
  border: 0px solid yellow;
  border-right: 10px solid #ffd740;
  min-height: 25px;
  font-size: 15px;
  background-color: #262626;
  color: #ffd740;
}
QComboBox:hover#comboLangue {
    background-color: #64ffda;
  color: #262626;
  padding: 0px;
  border: 0px solid transparent;
}
QComboBox:hover#comboTheme {
    background-color: #ffd740;
  color: #262626;
  padding: 0px;
  border: 0px solid transparent;
}
QComboBox:drop-down {
  border: 0px;
}
QComboBox QAbstractItemView {
  color: white;
  background-color: #1a191d;
  border: 0px solid white;
}
QComboBox#comboLangue QAbstractItemView::item {
  min-height: 25px;
  color: green;
}
